<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
<display-name>Archetype Created web Application</display-name>
<servlet>
  <servlet-name>HttpServletDemo</servlet-name>
  <servlet-class>ServletPackage.HttpServletDemo </servlet-class><!-- packagename.classname -->  
  </servlet>

  <servlet-mapping>
   <servlet-name>HttpServletDemo </servlet-name>
   
   <url-pattern>/HttpServlet</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  <servlet-name>GenericServletDemo</servlet-name>
  <servlet-class>ServletPackage.GenericServletDemo </servlet-class><!-- packagename.classname -->  
  </servlet>

  <servlet-mapping>
   <servlet-name>GenericServletDemo </servlet-name>
   
   <url-pattern>/GenericServletDemo</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>ServletInterfaceDemo</servlet-name>
  <servlet-class>ServletPackage.ServletInterfaceDemo </servlet-class><!-- packagename.classname -->  
  </servlet>

  <servlet-mapping>
   <servlet-name>ServletInterfaceDemo </servlet-name>
   
   <url-pattern>/ServletInterfaceDemo</url-pattern>
  </servlet-mapping>
</web-app>